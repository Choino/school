<script type="text/javascript">
    
    function ga_section_click( evt ) {
        try {
            var parent = $(this).closest(".ga_section");
            var section = parent.attr("name");
            
            var nth,
                a = this;
            
            if(parent.length)
                $("a", parent).each(function(n){
                    if( this === a )
                        nth = n;
                });
            
            var url = this.href.replace(/(http:\/\/[a-z]+\.theonion\.com)/, "");
            if (this.href.match(/(http:\/\/[a-z]+\.theonion\.com)/)) {
                _gaq.push(['_trackEvent', 'click', section, url]);
                analytics.trackClick(evt, section, nth, url);
                // console.log("CLICK section: " + section + " url: " + url);
            }
            else {
                _gaq.push(['_trackEvent', 'click-outbound', section, url]);
                // console.log("CLICK-OUTBOUND section: " + section + " url: " + url);
            }
        }
        catch (err) {}
        return true;
        // return false;
    }
    //only for onion, not sportsdome...(for now)
    if (window.location.href.match(/(http:\/\/[a-z]+\.theonion\.com)/)) {
        $("a").click(ga_section_click);
    }

    function extractParamFromUri(uri, paramName) {
      if (!uri) {
        return;
      }
      var uri = uri.split('#')[0];  // Remove anchor.
      var parts = uri.split('?');  // Check for query params.
      if (parts.length == 1) {
        return;
      }
      var query = decodeURI(parts[1]);

      // Find url param.
      paramName += '=';
      var params = query.split('&');
      for (var i = 0, param; param = params[i]; ++i) {
        if (param.indexOf(paramName) === 0) {
          return unescape(param.split('=')[1]);
        }
      }
    }
</script>
