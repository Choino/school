<div>
<script>
$(function(){
    var title = $('#connectbox .title');
    var forward_button = $('#connectbox .forward');
    var back_button = $('#connectbox .back');
    var articles = [];
    var currentIndex = 0;
    
    function nextHeadline(){
        if(currentIndex >= articles.length-1) return false;
        title.html(articles[++currentIndex].title);
        if(currentIndex >= articles.length-1) forward_button.addClass('disabled');
        if(currentIndex > 0) back_button.removeClass('disabled');
        return false;
    }
    function prevHeadline(){
        if(currentIndex <= 0) return false;
        title.html(articles[--currentIndex].title);
        if(currentIndex == 0) back_button.addClass('disabled');
        if(currentIndex < articles.length-1) forward_button.removeClass('disabled');
        return false;
    }
    $.getJSON('/ajax/more_news/', function(data) {
        articles = data;
        currentIndex = 0;
        title.html(articles[currentIndex].title);
        if(currentIndex < articles.length-1) forward_button.removeClass('disabled');
        forward_button.click(nextHeadline);
        back_button.click(prevHeadline);
    });
    

});
</script>
<!-- start connect box -->
