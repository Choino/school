<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/more_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="20dp"
        android:text="username"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="24dp" />

    <TextView
        android:id="@+id/more_realname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/more_username"
        android:layout_below="@+id/more_username"
        android:text="realname"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/more_realname"
        android:layout_below="@+id/more_realname"
        android:layout_marginTop="22dp"
        android:text="Tweets:"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/more_tweet_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView2"
        android:layout_alignBottom="@+id/textView2"
        android:layout_toRightOf="@+id/textView2"
        android:paddingLeft="25dp"
        android:text="3q34"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_toLeftOf="@+id/more_tweet_num"
        android:text="Followers:"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/more_followers_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView01"
        android:layout_alignBottom="@+id/TextView01"
        android:layout_alignLeft="@+id/more_tweet_num"
        android:paddingLeft="25dp"
        android:text="324324"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/TextView01"
        android:layout_below="@+id/TextView01"
        android:text="Following"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/more_friends_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView4"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignLeft="@+id/more_followers_num"
        android:paddingLeft="25dp"
        android:text="434234t"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <Button
        android:id="@+id/more_url1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignRight="@+id/textView6"
        android:layout_alignTop="@+id/more_hasURL"
        android:layout_marginTop="19dp"
        android:text="Button"
        android:textSize="10dp" />
    <TextView android:id="@+id/textView6" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="93dp" android:layout_marginRight="29dp"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TextView01"
        android:layout_below="@+id/textView4"
        android:layout_marginTop="19dp"
        android:text="Tweet:"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/more_hasURL"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/textView5"
        android:layout_marginBottom="93dp"
        android:layout_marginRight="29dp"
        android:text="safsfsfasf" />

    <TextView
        android:id="@+id/more_hasURL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/more_url1"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="93dp"
        android:text="URLs in Tweet:"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <Button
        android:id="@+id/more_url2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/more_url1"
        android:layout_alignRight="@+id/more_url1"
        android:layout_below="@+id/more_url1"
        android:text="Button"
        android:textSize="10dp" />

    <Button
        android:id="@+id/more_url3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/more_url2"
        android:layout_alignRight="@+id/more_url2"
        android:layout_below="@+id/more_url2"
        android:text="Button"
        android:textSize="10dp" />

    <TextView
        android:id="@+id/more_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/more_url1"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignRight="@+id/more_url1"
        android:layout_below="@+id/textView5"
        android:text="3842" />

    <Button
        android:id="@+id/more_image"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView5"
        android:layout_alignRight="@+id/more_content"
        android:text="View Image!" />

</RelativeLayout>